<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.skole.s304114mappe2ny.Fragmenter.NotifikasjonFragment">


    <Switch
        android:id="@+id/notifikasjonPaAv"
        android:layout_marginTop="100dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:text="Notifikasjon for resturantbestilling"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/klokkeslettNtf"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/notifikasjonPaAv"
        android:layout_centerHorizontal="true"
        android:textSize="18sp"
        android:text="Ingen notifikasjon"
        />



    <Switch
        android:id="@+id/meldingPaAv"
        android:layout_marginTop="27dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/klokkeslettNtf"
        android:layout_centerHorizontal="true"
        android:layout_centerVertical="true"
        android:text="Varselmelding"
        android:textSize="18sp" />

    <TextView
        android:id="@+id/klokkeslettmld"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/meldingPaAv"
        android:layout_centerHorizontal="true"
        android:textSize="18sp"
        android:text="Ingen varselmelding"
        />

    <Button
        android:layout_marginTop="27dp"
        android:id="@+id/btnTilbake"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@id/klokkeslettmld"
        android:layout_centerHorizontal="true"
        android:text="Tilbake" />




</RelativeLayout>