<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical" >

    <TextView
        android:id="@+id/textView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="@string/overskrift"
        android:textAlignment="center"
        android:textSize="24sp" />


    <Button
        android:id="@+id/resturanterKnapp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Resturanter" />


    <Button
        android:id="@+id/vennerKnapp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Venner" />

    <Button
        android:id="@+id/bestillingerKnapp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Bestilling" />


    <Button
        android:id="@+id/innstillingerKnapp"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Innstillinger" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Notifikasjon" />

</LinearLayout>